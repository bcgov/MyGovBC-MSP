{
  "name": "mygovbc-msp",
  "version": "3.3.2",
  "description": "MyGovBC MSP Service Provider Web App",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/MyGovBC-MSP.git"
  },
  "homepage": "https://my.gov.bc.ca/msp",
  "main": "server.js",
  "scripts": {
    "prestart": "npm install",
    "preinstall": "rm -rf node_modules/",
    "start": "npm run dev",
    "dev": "ng serve --host=\"0.0.0.0\" --port=4304 -o",
    "dev-build": "ng build",
    "prebuild": "node src/version.js",
    "build": "ng build --prod",
    "test": "ng test",
    "pree2e": "npx webdriver-manager update --versions.chrome 86.0.4240.198",
    "e2e": "ng e2e --no-webdriver-update",
    "e2e:all": "npm run e2e -- --protractorConfig=e2e/protractor.full-tests.conf.js",
    "e2e:chrome": "npm run e2e -- --protractorConfig=e2e/protractor.conf.js",
    "e2e:account": "npm run e2e -- --protractorConfig=e2e/protractor.account.conf.js",
    "e2e:enrolment": "npm run e2e -- --protractorConfig=e2e/protractor.enrolment.conf.js",
    "e2e:supp-benefits": "npm run e2e-protractor -- e2e/protractor.supp-benefits.conf.js",
    "e2e:retro-pa": "npm run e2e-protractor -- e2e/protractor.retro-pa.conf.js"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.10",
    "@angular/compiler-cli": "^7.2.16",
    "@types/jasmine": "2.8.9",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.11.5",
    "@types/pdfjs-dist": "^0.1.2",
    "async": "^2.6.1",
    "autoprefixer": "^6.0.0",
    "codelyzer": "~4.2.1",
    "core-js": "^2.5.7",
    "cross-env": "^3.1.3",
    "es6-promise": "^4.2.5",
    "eslint": "3.10.2",
    "font-awesome": "^4.7.0",
    "jasmine-core": "^3.6.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-spec-reporter": "0.0.32",
    "pdfjs-dist": "^2.0.550",
